<% include ./partials/header.ejs %>
    <div class="w-full min-h-screen bg-zinc-800 px-10 py-8 text-white">
        <div class="cards flex flex-wrap gap-10 ">
            <% posts.forEach(function(elem){ %>
                <div class="card w-44">
                    <div class="w-full h-40 bg-zinc-700 rounded-lg overflow-hidden">
                        <img src="/images/uploads/<%= elem.image%>" class="w-full h-full" alt="">
                    </div>
                    <h5 class="mt-2 text-sm">
                        <%= elem.title%>
                    </h5>
                    <h5 class="mt-2 font-semibold text-xs">
                        @<%= elem.user.username%>
                    </h5>
                </div>
                <% }) %>
        </div>
    </div>
    <% include ./partials/footer.ejs %>